{
  "name": "oracle-agent",
  "version": "6.0.0",
  "description": "Oracle Agent - Digital Partner for Tars (Phase 9: Practical AGI + Unified Memory)",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "sync": "node scripts/sync.js full",
    "sync:status": "node scripts/sync.js status",
    "sync:pull": "node scripts/sync.js pull",
    "sync:push": "node scripts/sync.js push",
    "hotel": "node scripts/sync.js hotel",
    "memory": "node scripts/memory-sync.js",
    "memory:sync": "node scripts/memory-sync.js sync",
    "memory:status": "node scripts/memory-sync.js status",
    "memory:search": "node scripts/memory-sync.js search",
    "boot": "node -e \"import('./lib/boot-system.js').then(m => m.runBoot())\"",
    "summarize": "node tools/summarize-session.js",
    "summarize:all": "node tools/summarize-session.js --all",
    "summarize:terminal": "node tools/terminal-summarizer.js",
    "summarize:terminal:all": "node tools/terminal-summarizer.js --all",
    "summarize:terminal:list": "node tools/terminal-summarizer.js --list"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "openai": "^4.20.0",
    "p-queue": "^8.0.1",
    "pg": "^8.18.0",
    "uuid": "^9.0.0",
    "ws": "^8.19.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.4.0"
  },
  "devDependencies": {
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
